    <link rel="stylesheet" type="text/css" href="{{siteUrl}}assets/css/all.css">
  </head>
  <body>
    <!-- change -->
    <div class="container-fluid" style="background-color:#444444;">
      <nav class="navbar navbar-toggleable-md bg-dark navbar-inverse container" style="padding-left:0;padding-right:0;">
        <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarText" aria-controls="navbarText" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <a class="navbar-brand" href="{{siteUrl}}">Chris Orchard</a>
        <a class="navbar-brand navbar-brand-subtitle"  href="{{siteUrl}}">Registered Valuer</a>
        <div class="collapse navbar-collapse navbar-fullscreen" id="navbarText">
          <ul class="navbar-nav ml-auto">
            <li class="nav-item">
              <a class="nav-link nav__text" href="{{siteUrl}}about">About <span class="sr-only">(current)</span></a>
            </li>
            <li class="nav-item">
              <a class="nav-link nav__text" href="{{siteUrl}}services">Services</a>
            </li>
            <li class="nav-item">
              <a class="nav-link nav__text" href="{{siteUrl}}experience">Experience</a>
            </li>
            <li class="nav-item">
              <a class="nav-link nav__text" href="{{siteUrl}}employment">Employment</a>
            </li>
            <li class="nav-item">
              <a class="nav-link nav__text" href="{{siteUrl}}contact">Contact</a>
            </li>
          </ul>
        </div>
      </nav>
    </div>

    <div class="container-fluid d-flex align-items-center home__background__img index__hero__main py-5">
      <div class="container">
        <div class="row justify-content-start">
          <div class="col-md-6 index__hero__main__text__column">
            <h1 class="index__hero__main__text text-right">{{ entry.homeHero.one.mainHeader }}</h1>
            <a href="{{siteUrl}}contact" class="btn btn-primary btn-lg float-right">{{ entry.homeHero.one.buttonText }}</a>
          </div>
        </div>
      </div>
    </div>
    <div class="container-fluid footer__bg py-5">
      <div class="container ">
        <div class="row">
          <div class="col-md-6 mt-auto text-white d-flex justify-content-center justify-content-md-start py-3">
            <img src="{{siteUrl}}assets/images/logo-min.jpg" style="width:7.80rem;height:2rem;text-align:center;" class=" " alt=""></img>
          </div>
          <div class="col-md-6 text-white text-center text-md-right">
            {% for item in contact.contactDetails %}
              {% if item.heading != "Physical Address" %}
                <p>
                  <span class="footer__contact__label">{{ item.heading }}</span><br>
                  <a class="footer__contact__label__link">{{ item.detail }}</a>
                </p>
              {% endif %}
            {% endfor %}
          </div>
        </div>
      </div>
    </div>
</body>
</html>
